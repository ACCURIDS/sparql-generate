BASE <http://example.org/> 
PREFIX rqg-ite: <http://w3id.org/sparql-generate/ite/>
PREFIX rqg-fn: <http://w3id.org/sparql-generate/fn/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
GENERATE { 
  ?personNode a foaf:Person .
    ?personNode foaf:name ?personName .
    GENERATE {
        ?personNode foaf:knows ?knowsPersonNode .
        ?knowsPersonNode a foaf:Person .
        ?knowsPersonNode foaf:name ?knowsPerson .
    }
    ITERATOR rqg-ite:XPath(?person, "//knows" ) AS ?knows 
    WHERE {
        BIND( rqg-fn:XPath(?knows,"/knows"  ) AS ?knowsPerson )
        BIND (BNODE(?knowsPerson) AS ?knowsPersonNode)
    } .
}
SOURCE <http://example.com/relations.xml> AS ?source
ITERATOR rqg-ite:XPath(?source, "//person" ) AS ?person
WHERE {
    BIND( rqg-fn:XPath(?person,"/person/@name"  ) AS ?personName )
    BIND (BNODE(?personName) AS ?personNode)
}