BASE <http://example.org/> 
PREFIX ite: <http://w3id.org/sparql-generate/ite/>
PREFIX fn: <http://w3id.org/sparql-generate/fn/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX ex: <http://www.example.com/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX schema: <http://schema.org/>
PREFIX wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>
PREFIX geosp: <http://www.telegraphis.net/ontology/geography/geography#>
PREFIX gn: <http://www.geonames.org/ontology#>
PREFIX skos:   <http://www.w3.org/2004/02/skos/core#>
GENERATE { 
      [] a skos:Concept;
            skos:prefLabel ?prefLabel;
            skos:altLabel ?altLabel .
}
SOURCE <http://example.com/thesaurus.xml> AS ?source
ITERATOR ite:XPath(?source,"//Localizations") AS ?localization
WHERE {
   BIND( STRLANG(fn:XPath(?localization,'Localizations/Localization[@Culture="nl-BE"]/Label/text()'),"en") AS ?altLabel )
   BIND( STRLANG(fn:XPath(?localization,'Localizations/Localization[@Culture="en"]/Label/text()'),"nl") AS ?prefLabel  )

 
}