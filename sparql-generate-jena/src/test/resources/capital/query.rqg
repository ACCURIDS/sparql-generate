BASE <http://example.org/> 
PREFIX ite: <http://w3id.org/sparql-generate/ite/>
PREFIX fn: <http://w3id.org/sparql-generate/fn/>

GENERATE { 
  [] a <Country> ;
    <code> ?key ;
    <capital> ?capital.

}
SOURCE <http://country.io/capital.json> AS ?source
ITERATOR ite:JSONListKeys(?source) AS ?key
WHERE {
   FILTER(STRSTARTS(?key,"F"))
   BIND(CONCAT('$.', ?key) AS ?query)
   BIND(fn:JSONPath(?source, ?query ) AS ?capital)
}
ORDER BY ?key
LIMIT 3
