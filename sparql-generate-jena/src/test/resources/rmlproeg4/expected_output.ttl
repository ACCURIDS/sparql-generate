@prefix schema: <http://schema.org/> .
@prefix geosp: <http://www.telegraphis.net/ontology/geography/geography#> .
@prefix gn:    <http://www.geonames.org/ontology#> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

<http://loc.example.com/city/Gent>
        a                  schema:City ;
        gn:countryCode     "BE" ;
        geosp:onContinent  "EU" ;
        wgs84_pos:lat      "51.4657890" ;
        wgs84_pos:long     "51.4657890" .

<http://loc.example.com/city/Brussels>
        a                  schema:City ;
        gn:countryCode     "BE" ;
        geosp:onContinent  "EU" ;
        wgs84_pos:lat      "51.0000000" ;
        wgs84_pos:long     "51.0000000" .
