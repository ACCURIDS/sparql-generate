PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX sgiter: <http://w3id.org/sparql-generate/iter/>
PREFIX sgfn: <http://w3id.org/sparql-generate/fn/>

GENERATE { 
  <http://ex.org/project/{ sgfn:CustomCSV( ?project, "id", '"' , ";" , "\n" , true ) }>
    dcterms:title "{ sgfn:CustomCSV( ?project, "acronym", '"' , ";" , "\n" , true ) }"@en ;
    dcterms:description "{ sgfn:CustomCSV( ?project, "objective", '"' , ";" , "\n" , true ) }"@en .
}
ITERATOR sgiter:CSVStream(<http://www.emse.fr/~zimmermann/Data/cordis-h2020projects.10lines.csv>, true, 2, 0) AS ?project
