BASE <http://example.com/> 
PREFIX ite: <http://w3id.org/sparql-generate/iter/>
PREFIX fun: <http://w3id.org/sparql-generate/fn/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX ex: <http://example.com/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX schema: <http://schema.org/>
PREFIX wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>
PREFIX geosp: <http://www.telegraphis.net/ontology/geography/geography#>
PREFIX gn: <http://www.geonames.org/ontology#>
PREFIX skos:   <http://www.w3.org/2004/02/skos/core#>

# example from https://github.com/RMLio/RML-Processor/tree/master/src/test/resources
GENERATE { 
      <http://example.com/concept/{ ?enLabel }> a skos:Concept;
            skos:prefLabel "{ ?enLabel }"@en ;
            skos:altLabel "{ ?nlLabel }"@nl .

}
ITERATOR ite:XPath(<http://example.com/thesaurus.xml>, 
                    '/Thesauri/Thesaurus/Terms/Term/Localizations', 
                    '/Localizations/Localization[@Culture="en"]/Label/text()',
                    '/Localizations/Localization[@Culture="nl-BE"]/Label/text()'
                    ) AS 
                     ?localizations
                     ?enLabel 
                     ?nlLabel