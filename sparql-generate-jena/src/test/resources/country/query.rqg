BASE <http://example.org/> 
PREFIX ite: <http://w3id.org/sparql-generate/ite/>
PREFIX fn: <http://w3id.org/sparql-generate/fn/>

GENERATE { 

  _:country a <Country> ;
    <key> ?key .

    # deliberately use a stupid join
    # this will be a new blank node
    GENERATE {
      _:country <capital> ?capital.
    }
    SOURCE <http://country.io/capital.json> AS ?source2
    ITERATOR ite:JSONListKeys(?source) AS ?key2
     WHERE {
      FILTER (?key=?key2)
      BIND(CONCAT('$.', ?key2) AS ?query2)
      BIND(fn:JSONPath(?source2, ?query2 ) AS ?capital)
    } .

  # this must be the same blank node
  _:country a <Country> ;
    <name> ?name.

}
SOURCE <http://country.io/names.json> AS ?source
ITERATOR ite:JSONListKeys(?source) AS ?key
WHERE {
   FILTER(STRSTARTS(?key,"F"))
   BIND(CONCAT('$.', ?key) AS ?query)
   BIND(fn:JSONPath(?source, ?query ) AS ?name)
}
ORDER BY ?key
LIMIT 10
OFFSET 1
