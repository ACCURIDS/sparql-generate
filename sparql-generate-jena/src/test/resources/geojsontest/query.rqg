BASE <http://example.com/> 
PREFIX iter: <http://w3id.org/sparql-generate/iter/>
PREFIX fun: <http://w3id.org/sparql-generate/fn/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

PREFIX wkt: <http://www.opengis.net/ont/geosparql#>

PREFIX ex: <http://example.com/>

GENERATE {
	?regionIRI a ex:Region;
		wkt:asWKT ?geometricCoordinates .
}
SOURCE <https://raw.githubusercontent.com/NetTopologySuite/NetTopologySuite.IO.GeoJSON/develop/NetTopologySuite.IO.GeoJSON.Test/World_AL6.GeoJson> AS ?source

#ITERATOR iter:GeoJSONFeatures(?source) AS ?region
#WHERE {
#	BIND (IRI(CONCAT("http://region.example.com/",fun:JSONPath(?region,"$.id"))) as ?regionIRI)
#	BIND (fun:GeoJSONGeometry(?region) AS ?geometricCoordinates)
#}

ITERATOR iter:GeoJSON(?source) AS ?geometricCoordinates ?properties
WHERE {
    BIND (URI(CONCAT("http://region.example.com/",fun:JSONPath(?properties,"$.id"))) AS ?regionIRI)
}