BASE <http://example.com/> 
PREFIX ite: <http://w3id.org/sparql-generate/iter/>
PREFIX fun: <http://w3id.org/sparql-generate/fn/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX transit: <http://vocab.org/transit/terms/>

GENERATE {
  ?airportURI a transit:Stop;
        transit:coordinates1 ?geometryWkt;
        transit:coordinates2 ?latlongWkt.
}
SOURCE <http://example.com/airport.csv> AS ?source
ITERATOR ite:CSVMultipleOutputs(?source,";","id","latitude","longitude","wkt") AS ?id ?latitude ?longitude ?geometry
WHERE {
    BIND (URI(CONCAT("http://airport.example.com/",?id)) AS ?airportURI)
    BIND(fun:GeoCSVasWKT(?latitude,?longitude) AS ?latlongWkt)
    BIND(fun:GeoCSVasWKT(?geometry) AS ?geometryWkt)
}