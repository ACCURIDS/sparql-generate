BASE <http://example.org/> 
PREFIX iter: <http://w3id.org/sparql-generate/iter/>
PREFIX fn: <http://w3id.org/sparql-generate/fn/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX bibo:    <http://purl.org/ontology/bibo/> 
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX spc:     <http://semweb.mmlab.be/ns/spc/>
GENERATE {  
  ?volumeIRI a bibo:Volume;
                dcterms:identifier ?identifier;
                bibo:shortTitle ?shortTitle;
                rdfs:label ?fullTitle;
                spc:fullTitle ?fullTitle;
                rdfs:label ?volTitle;
                spc:volTitle ?volTitle .
}
SOURCE <http://ceur-ws.org/Vol-1128/index.html> AS ?source
ITERATOR iter:CSSPath(?source, "body" ) AS ?body 
WHERE {
	BIND(fn:HTMLTag(?body, "span.CEURVOLNR") AS ?volume )
        BIND ( IRI(CONCAT("http://ceur-ws.org/",?volume)) as ?volumeIRI)
	BIND(fn:HTMLTag(?body, "span.CEURURN") AS ?identifier )
	BIND(fn:HTMLTag(?body, "span.CEURVOLACRONYM") AS ?shortTitle )
	BIND(fn:HTMLTag(?body, "span.CEURFULLTITLE") AS ?fullTitle )

        BIND(fn:HTMLTag(?body, "span.CEURVOLTITLE") AS ?volTitle )
}