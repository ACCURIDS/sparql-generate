BASE <http://example.org/> 
PREFIX rqg-sel: <http://w3id.org/sparql-generate/sel/>
PREFIX rqg-fn: <http://w3id.org/sparql-generate/fn/>

GENERATE { 
  [] a <Country> ;
    <key> ?key ;
    <name> ?countryName .

} WHERE {
   BIND(CONCAT('$.', ?key) AS ?query)
   BIND(rqg-fn:JSONPath_jayway_string(?source, ?query ) AS ?countryName)
}
SELECTOR rqg-sel:JSONListKeys(?source) AS ?key
SOURCE <http://country.io/capital.json> AS ?source
